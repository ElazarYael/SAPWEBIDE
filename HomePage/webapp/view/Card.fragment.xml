<core:FragmentDefinition xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:html="http://www.w3.org/1999/xhtml"  xmlns:f="sap.f" xmlns:m="sap.m" xmlns:card="sap.f.cards"
	xmlns:core="sap.ui.core" displayBlock="true" xmlns:ObjP="sap.uxap" height="100%">

	
	<f:Card>
		<f:content>
			<HBox alignItems="Center" justifyContent="SpaceBetween" fitContainer="true">
				<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom">
					<Image press="Onclick"
						src="https://sapui5.hana.ondemand.com/test-resources/sap/m/demokit/sample/ImageContent/images/SAPLogoLargeTile_28px_height.png"></Image>
				</VBox>
						
			
				<List
					showSeparators="None" >
					<CustomListItem>
						<HBox
							 alignItems="Center"
							justifyContent="SpaceBetween">
							<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
								<Title  text="title" />
								<Text text="subtitle"/>
								<Label text="name"/>
								<Label text="last name"/>
							</VBox>
							<Image press="Onclick"
						src="https://sapui5.hana.ondemand.com/test-resources/sap/m/demokit/sample/ImageContent/images/SAPLogoLargeTile_28px_height.png"></Image>
							<!--<ObjectStatus
								class="sapUiTinyMargin"
								text="revenue"
								state= "{statusSchema}"/>-->
						</HBox>
					</CustomListItem>
				</List>
			
		
	
			</HBox>
		</f:content>
	</f:Card>
</core:FragmentDefinition>